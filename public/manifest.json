{
    "manifest_version": 3,
    "name": "QuickSelect",
    "version": "1.0",
    "description": "Build with React and webpack",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting",
      "storage"
    ],
    "background": {
      "service_worker": "background.bundle.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.bundle.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "popup.html",
          "popup.bundle.js"
      ],
        "matches": ["<all_urls>"]
      }
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }
  